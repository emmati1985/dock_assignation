<!--xmlns:t="sap.ui.table"-->
<mvc:View xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic"
	controllerName="dockass.controller.Detail">
	<semantic:DetailPage id="page" navButtonPress="onNavBack" showNavButton="{device>/system/phone}" title="{i18n>detailTitle}"
		busy="{detailView>/busy}" busyIndicatorDelay="{detailView>/delay}">
		<semantic:content>
			<ObjectHeader id="objectHeader" title="{dock}"></ObjectHeader>
			<l:VerticalLayout>
				<Image class="sapUiSmallMargin" src="http://emccochin.com/wp-content/uploads/2016/04/work-in-progress_01.png" height="10%" width="10%"/>
				<l:content>
					<Table id="Table" width="auto" items="{path:'assignation', sorter: { path: 'Dock' }}">
						<columns>
							<Column id="column0">
								<header>
									<Label id="label0" text="Dock"/>
								</header>
							</Column>
							<Column id="column1">
								<header>
									<Label id="label1" text="H1"/>
								</header>
							</Column>
							<Column id="column2">
								<header>
									<Label id="label2" text="H2"/>
								</header>
							</Column>
							<Column id="column3">
								<header>
									<Label id="label3" text="H3"/>
								</header>
							</Column>
							<Column id="column4">
								<header>
									<Label id="label4" text="H4"/>
								</header>
							</Column>
							<Column id="column5">
								<header>
									<Label id="label5" text="H5"/>
								</header>
							</Column>
							<Column id="column6">
								<header>
									<Label id="label6" text="H6"/>
								</header>
							</Column>
							<Column id="column7">
								<header>
									<Label id="label7" text="H7"/>
								</header>
							</Column>
							<Column id="column8">
								<header>
									<Label id="label8" text="H8"/>
								</header>
							</Column>
						</columns>
						<items id="items">
							<ColumnListItem id="collist">
								<cells>
									<ObjectIdentifier title="{Dock}"/>
									<ObjectStatus id="stat1" icon="{= ${h1} === false ? 'sap-icon://message-error' : 'sap-icon://message-success' }" state="{= ${h1} === false ? 'Error' : 'Success' }"/>
									<ObjectStatus id="stat2" icon="{= ${h2} === false ? 'sap-icon://message-error' : 'sap-icon://message-success' }" state="{= ${h2} === false ? 'Error' : 'Success' }"/>
									<ObjectStatus id="stat3" icon="{= ${h3} === false ? 'sap-icon://message-error' : 'sap-icon://message-success' }" state="{= ${h3} === false ? 'Error' : 'Success' }"/>
									<ObjectStatus id="stat4" icon="{= ${h4} === false ? 'sap-icon://message-error' : 'sap-icon://message-success' }" state="{= ${h4} === false ? 'Error' : 'Success' }"/>
									<ObjectStatus id="stat5" icon="{= ${h5} === false ? 'sap-icon://message-error' : 'sap-icon://message-success' }" state="{= ${h5} === false ? 'Error' : 'Success' }"/>
									<ObjectStatus id="stat6" icon="{= ${h6} === false ? 'sap-icon://message-error' : 'sap-icon://message-success' }" state="{= ${h6} === false ? 'Error' : 'Success' }"/>
									<ObjectStatus id="stat7" icon="{= ${h7} === false ? 'sap-icon://message-error' : 'sap-icon://message-success' }" state="{= ${h7} === false ? 'Error' : 'Success' }"/>
									<ObjectStatus id="stat8" icon="{= ${h8} === false ? 'sap-icon://message-error' : 'sap-icon://message-success' }" state="{= ${h8} === false ? 'Error' : 'Success' }"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</l:content>
			</l:VerticalLayout>
		</semantic:content>
	</semantic:DetailPage>
</mvc:View>